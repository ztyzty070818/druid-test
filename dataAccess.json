{
  "type": "lucene_index",
  "worker": "dev224.sugo.net:8091",
  "spec": {
    "dataSchema": {
      "dataSource": "csv-test",
      "metricsSpec": [],
      "parser": {
        "parseSpec": {
          "format": "csv",
          "timestampSpec": {"column": "dt","format": "millis"},
          "dimensionsSpec": {
            "dimensionExclusions": [],
            "spatialDimensions": [],
            "dimensions": [
              {"name": "age","type": "int"},
              {"name": "province","type": "string"},
              {"name": "birthday","type": "date","format":"millis"},
              {"name": "salary","type": "long"},
              {"name": "height","type": "double"},
              {"name": "average","type": "float"}
            ]
          },
          "listDelimiter": ",",
          "columns": ["age","province","dt","birthday","salary","height","average"]
        }
      },
      "granularitySpec": {
        "intervals": ["2017-05-01/2017-06-10"],
        "segmentGranularity": "DAY",
        "queryGranularity": "NONE",
        "type": "uniform"
      }
    },
    "ioConfig": {
      "type":"lucene_index",
      "firehose":{
        "type": "local",
        "filter": "data.csv",
        "baseDir": "/data1/csv/"
      }
    },
    "tuningConfig": {
      "type": "lucene_index",
      "maxRowsPerSegment": 5000000,
      "overwrite": false,
      "reportParseExceptions":true
    }
  },
  "context":{
    "debug":true
  }
}